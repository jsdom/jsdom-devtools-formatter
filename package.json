{
  "name": "jsdom-devtools-formatter",
  "version": "1.0.0",
  "description": "Make jsdom elements look like real DOM elements in Chrome Devtools console",
  "repository": "https://github.com/jsdom/jsdom-devtools-formatter",
  "engines": {
    "node": ">=8.9.4"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "src"
  ],
  "scripts": {
    "test": "jest",
    "test:debug": "node --inspect=0.0.0.0 ./node_modules/.bin/jest --runInBand",
    "test:manual_jest": "node --inspect=0.0.0.0 ./node_modules/.bin/jest --runInBand manual-testing/jest",
    "test:manual_node": "node --inspect=0.0.0.0 manual-testing/node.js"
  },
  "keywords": [
    "jsdom",
    "chrome",
    "devtools",
    "console",
    "formatter"
  ],
  "author": "Nicklas Gummesson",
  "license": "MIT",
  "devDependencies": {
    "jest": "^22.4.2",
    "jsdom": "^11.6.2"
  },
  "dependencies": {}
}
